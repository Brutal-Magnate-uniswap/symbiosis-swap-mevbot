Package: bytemark-symbiosis
Architecture: all
Depends: symbiosis-key (>= 2012:0222), 
  symbiosis-updater (>= 2012:0222),
  ${misc:Depends}
Recommends: symbiosis-backup (>= 2012:0222),
  symbiosis-common (>= 2012:0222),
  symbiosis-cron (>= 2012:0222),
  symbiosis-email (>= 2012:0222),
  symbiosis-firewall (>= 2012:0222), 
  symbiosis-ftpd (>= 2012:0222),
  symbiosis-httpd (>= 2012:0222), 
  symbiosis-monit (>= 2012:0222), 
  symbiosis-mysql (>= 2012:0222),
  symbiosis-pam (>= 2012:0222), 
  symbiosis-phpmyadmin (>= 2012:0222), 
